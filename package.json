{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:lenient": "cross-env NO_STRICT_MODE=true yarn dev",
    "build": "next build",
    "start": "next start",
    "check-types": "tsc --noEmit",
    "lint": "eslint --fix \"src/**/*.{ts,tsx}\" && yarn check-types",
    "fix-type-exports": "node ./scripts/fixTypeExports.js",
    "delete-features-index": "rimraf src/features/*/index.ts",
    "generate:ogimage": "tsx scripts/generateMetadataImage.ts",
    "generate-features-index": "barrelsby --config .barrelsby.json",
    "generate-folder-index": "ctix ./tsconfig.json --useBackupFile false --useComment false --excludePath true && yarn fix-type-exports",
    "index": "yarn generate-folder-index && yarn delete-features-index && yarn generate-features-index",
    "precommit": "yarn lint",
    "commit": "git-cz",
    "analyze-bundle": "cross-env ANALYZE=true yarn build"
  },
  "dependencies": {
    "@mantine/core": "^4.1.4",
    "@mantine/form": "^4.2.8",
    "@mantine/hooks": "^4.1.4",
    "@mantine/next": "^4.1.4",
    "@mobily/ts-belt": "^3.11.0",
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.27.2",
    "chart.js": "^3.8.0",
    "eslint": "^8.16.0",
    "next": "^12.1.6",
    "next-plausible": "^3.2.0",
    "polished": "^4.2.2",
    "react": "^18.0.0",
    "react-chartjs-2": "^4.1.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.2",
    "reselect": "^4.1.6",
    "tabler-icons-react": "^1.48.1",
    "ts-pattern": "^4.0.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.6",
    "@playwright/test": "^1.23.0",
    "@types/node": "17.0.25",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "barrelsby": "^2.3.4",
    "cross-env": "^7.0.3",
    "ctix": "^0.6.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "git-cz": "^4.9.0",
    "prettier": "^2.6.2",
    "replace-in-file": "^6.3.5",
    "rimraf": "^3.0.2",
    "tsx": "^3.6.0",
    "type-fest": "^2.13.0",
    "typescript": "4.7.3"
  }
}
